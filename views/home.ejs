<%- include('partials/header') -%>

<h1> <%= pageTitle %> </h1>

<p> <%= startingContent %> </p>

<ul>
    
    <% for (let i = 0; i < posts.length; i++) { %>
        <a style=" text-decoration: none;" href="/posts/<%= i%>">
            <li class="post">
                <h1><%= posts[i].title %></h1>
                <p><%= posts[i].content %></p>
            </li>
        </a>
    <% } %>
    
</ul>



<%- include('partials/footer') -%>